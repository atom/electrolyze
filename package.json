{
  "name": "electron-link",
  "version": "0.4.3",
  "description": "Generates scripts to pass to V8's `mksnapshot` tool.",
  "main": "lib/index.js",
  "scripts": {
    "test": "mocha test/**/*.test.js --ui=tdd --require @babel/register",
    "tdd": "electron-mocha test/**/*.test.js --ui=tdd --renderer --interactive --require @babel/register",
    "build": "rimraf lib && babel src -d lib",
    "prepare": "npm run build"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@babel/cli": "^7.10.3",
    "@babel/plugin-transform-async-to-generator": "^7.10.1",
    "@babel/register": "^7.10.3",
    "dedent": "0.7.0",
    "electron": "7.2.4",
    "electron-mocha": "^8.2.2",
    "mocha": "^8.0.1",
    "rimraf": "3.0.2",
    "temp": "0.9.1"
  },
  "dependencies": {
    "acorn": "^7.3.1",
    "ast-util-plus": "^0.6.2",
    "encoding-down": "^6.3.0",
    "indent-string": "^4.0.0",
    "leveldown": "^5.6.0",
    "levelup": "^4.4.0",
    "recast": "^0.19.1",
    "resolve": "^1.17.0",
    "source-map": "^0.7.3"
  }
}
